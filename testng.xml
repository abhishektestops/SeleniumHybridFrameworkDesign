<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	<groups>
		<run>
			<include name="Regression"></include>
			<include name="ErrorValidations"></include>
		</run>
	</groups>
	<test thread-count="5" name="SubmitOrderTest">
		<classes>
			<class
				name="abhishektestnet.seleniumFrameworkDesign.tests.SubmitOrderTest" />
		</classes>
	</test> <!-- Test -->
	
	<test thread-count="5" name="LoginErrorValidationTest">
		<classes>
			<class
				name="abhishektestnet.seleniumFrameworkDesign.tests.LoginErrorValidationTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
